Host *
  ForwardAgent yes
  ForwardX11 no
  CheckHostIP yes
  StrictHostKeyChecking no
  GSSAPIAuthentication no
  TCPKeepAlive yes
  ServerAliveInterval 30
  ServerAliveCountMax 3
  ControlMaster auto
  ControlPath ~/.ssh/%r@%h:%p
  UseRoaming no
