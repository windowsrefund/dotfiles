  Hostname nycpatriot.org
  Port 5554
  # 20180911 - See systemd --user list-unit-files
  RemoteForward 7777 localhost:22
  LocalForward 8025 localhost:25
  ServerAliveInterval 30
  ServerAliveCountMax 3
